{
  "dashboard": {
    "title": "AWS SQS Observability Dashboard",
    "tags": ["sqs", "aws", "observability"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Messages Sent",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(sqs_messages_sent_total[5m])",
            "legendFormat": "{{queue_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Messages Received",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(sqs_messages_received_total[5m])",
            "legendFormat": "{{queue_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Processing Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(sqs_message_processing_duration_bucket[5m]))",
            "legendFormat": "95th percentile - {{queue_name}} - {{operation}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(sqs_message_processing_duration_bucket[5m]))",
            "legendFormat": "50th percentile - {{queue_name}} - {{operation}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Queue Size",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sqs_queue_size",
            "legendFormat": "{{queue_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "min": 0,
          "max": 1000
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(sqs_messages_sent_total{status=\"error\"}[5m]) + rate(sqs_messages_received_total{status=\"error\"}[5m])",
            "legendFormat": "Errors/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Queues Created",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sqs_queue_created_total",
            "legendFormat": "{{queue_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  }
}
